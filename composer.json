{
    "name": "astina/social-links-bundle",
    "description": "Symfony bundle that is using SocialLinks to generate share links for social networks.",
    "keywords": ["astina", "social", "links", "share"],
    "type": "symfony-bundle",
    "homepage": "http://astina.ch",
    "authors": [
        {
            "name": "Astina AG",
            "email": "admin@astina.ch"
        }
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "require": {
        "symfony/symfony": "~2.3",
        "sensio/framework-extra-bundle": ">=2.3",
        "social-links/social-links": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*"
    },
    "autoload": {
        "psr-0": {
            "Astina\\Bundle\\SocialLinksBundle": ""
        }
    },
    "target-dir": "Astina/Bundle/SocialLinksBundle",
    "config": {
        "bin-dir": "bin"
    }
}
